#include <stdio.h>
int main() 
{
    char p1,p2;
    printf("Player 1, enter your choice (R,P,S): ");
    scanf(" %c", &p1);

    printf("Player 2, enter your choice (R,P,S): ");
    scanf(" %c",&p2);
    if (p1==p2) 
    {
        printf("It's a draw!\n");
    } 
    else if ((p1=='R'||p1=='r') && (p2=='S'||p2=='s')) 
    {
        printf("Player 1 wins! Rock breaks Scissors.\n");
    }
    else if ((p1=='P'||p1=='p') && (p2=='R'||p2=='r')) 
    {
        printf("Player 1 wins! Paper wraps Rock.\n");
    }
    else if ((p1=='S'||p1=='s') && (p2=='P'||p2=='p')) 
    {
        printf("Player 1 wins! Scissors cut Paper.\n");
    }
    else if ((p2=='R'||p2=='r') && (p1=='S'||p1=='s'))
    {
        printf("Player 2 wins! Rock breaks Scissors.\n");
    }
    else if ((p2=='P'||p2=='p')&&(p1=='R'||p1=='r'))
    {
        printf("Player 2 wins! Paper wraps Rock.\n");
    } 
    else if ((p2=='S'||p2=='s')&&(p1=='P'||p1=='p'))
    {
        printf("Player 2 wins! Scissors cut Paper.\n");
    }
    else
    {
        printf("Invalid!!!!!\n");
    }

    return 0;
}
