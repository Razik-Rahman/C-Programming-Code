#include <stdio.h>
int main() 
{
    int a,b;
    printf("Enter two numbers: ");
    scanf("%d%d",&a,&b);
    printf("\nBefore Swapping two numbers:");
    printf("\na=%d b=%d",a,b);
    a=a+b;
    b=a-b;
    a=a-b;
     printf("\nAfter Swapping two numbers:");
    printf("\na=%d b=%d",a,b);
    return 0;
}
